<atlassian-plugin name='Top Navigation Bar' key='jira.top.navigation.bar' i18n-name-key="top.navigation.plugin.name">
    <plugin-info>
        <description key="top.navigation.plugin.desc">This plugin renders the content of the top navigation bar in JIRA.
        </description>
        <vendor name="Atlassian" url="http://www.atlassian.com"/>
        <application-version min="3.12" max="3.12"/>
        <version>1.0</version>
    </plugin-info>

    <top-navigation key="standard-navigation-top" i18n-name-key="top.navigation.plugin.standard.name"
                    name="Standard Top Navigation Bar"
                    class="com.atlassian.jira.plugin.navigation.DefaultPluggableTopNavigation" state='enabled'>
        <resource type="velocity" name="view" location="templates/plugins/topnavigation/topnav.vm"/>
        <order>10</order>
    </top-navigation>


    <!--Dashboard Menu-->

    <web-item key="home_link" name="Home Link" section="system.top.navigation.bar"
              i18n-name-key="webfragments.navigation.bar.item.home.name" system="true" weight="10">
        <label key="menu.dashboard.header"/>
        <tooltip key="menu.dashboard.header.title"/>
        <link linkId="home_link" accessKey="$authcontext.i18nBean.getText('alt.text.home.accessKey')">
            /secure/Dashboard.jspa
        </link>
    </web-item>

    <web-section key="dashboard_link_main" name="Dashboard dropdown Main Section" location="home_link"
                 i18n-name-key="webfragments.navigation.bar.dash.dropdown.main" system="true" weight="10"/>

    <simple-link-factory key="dash_drop_fav-factory" name="Favourite Dashboard Link Factory"
                         section="home_link/dashboard_link_main"
                         i18n-name-key="webfragments.navigation.bar.dash.link.factory" system="true" weight="20"
                         lazy="true"
                         class="com.atlassian.jira.portal.FavouriteDashboardLinkFactory"/>

    <web-section key="dashboard_link_manage" name="Manage Dashboards Dropdown Section" location="home_link"
                 i18n-name-key="webfragments.navigation.bar.dash.dropdown.manage" system="true" weight="90">
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserLoggedInCondition"/>
    </web-section>

    <web-item key="manage_dashboard_drop" name="Manage Dashboards Link" section="home_link/dashboard_link_manage"
              i18n-name-key="webfragments.navigation.bar.dash.item.manage" system="true" weight="10">
        <label key="menu.dashboard.manage"/>
        <tooltip key="menu.dashboard.manage.title"/>
        <link linkId="manage_dash_link">/secure/ConfigurePortalPages!default.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserLoggedInCondition"/>
    </web-item>


    <!--Project Menu-->

    <!-- if there are any projects the user can browse - show them the browse link -->
    <web-item key="browse_project_link" name="Browse Project Link" section="system.top.navigation.bar"
              i18n-name-key="webfragments.navigation.bar.item.browse.project.name" system="true" weight="20">
        <label key="menu.project.header"/>
        <tooltip key="menu.project.header.selected.title">
            <param name="param0">$textutils.htmlEncode($helper.project.get('name'))</param>
        </tooltip>
        <link linkId="browse_link" accessKey="$authcontext.i18nBean.getText('alt.text.browseproject.accessKey')">
            /browse/$helper.project.get('key')
        </link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.HasSelectedProjectCondition"/>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserHasVisibleProjectsCondition">
            <param name="permission">browse</param>
        </condition>
    </web-item>

    <web-item key="browse_projects_link" name="Browse Projects Link" section="system.top.navigation.bar"
              i18n-name-key="webfragments.navigation.bar.item.browse.projects.name" system="true" weight="20">
        <label key="menu.project.header"/>
        <tooltip key="menu.project.header.all.title"/>
        <link linkId="browse_link" accessKey="$authcontext.i18nBean.getText('alt.text.browseproject.accessKey')">
            /secure/BrowseProjects.jspa
        </link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.HasSelectedProjectCondition" invert="true"/>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserHasVisibleProjectsCondition">
            <param name="permission">browse</param>
        </condition>
    </web-item>

    <web-section key="project_current" name="Project dropdown Current Project Section" location="browse_link"
                 i18n-name-key="webfragments.navigation.bar.browse.dropdown.current" system="true" weight="20">
        <label key="menu.project.current"/>
        <tooltip key="menu.project.current.desc"/>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.HasSelectedProjectCondition"/>
    </web-section>

    <web-item key="project_current_proj" name="Project dropdown Current Project Link"
              section="browse_link/project_current" i18n-name-key="webfragments.navigation.bar.browse.link.current"
              system="true" weight="20">
        <label>${helper.projectObject.name} (${helper.projectObject.key})</label>
        <link linkId="admin_main_proj_link">/browse/${helper.projectObject.key}</link>
        <icon width="16" height="16">
            <link linkId="admin_main_proj_link">/secure/projectavatar?pid=${helper.projectObject.id}&amp;avatarId=${helper.projectObject.avatar.id}&amp;size=small</link>
        </icon>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.HasSelectedProjectCondition"/>
    </web-item>

    <web-section key="project_history_main" name="Project dropdown history Section" location="browse_link"
                 i18n-name-key="webfragments.navigation.bar.browse.dropdown.history" system="true" weight="30">
        <label key="menu.project.recent"/>
        <tooltip key="menu.project.recent.desc"/>
    </web-section>

    <simple-link-factory key="project_drop_history-factory" name="Project History Link Factory"
                         section="browse_link/project_history_main"
                         i18n-name-key="webfragments.navigation.bar.browse.history.link.factory" system="true"
                         weight="20" lazy="true"
                         class="com.atlassian.jira.project.ProjectHistoryLinkFactory"/>

    <web-section key="project_view_all" name="View All projects Dropdown Section" location="browse_link"
                 i18n-name-key="webfragments.navigation.bar.project.dropdown.all" system="true" weight="90"/>

    <web-item key="project_view_all_link" name="View All Projects Link" section="browse_link/project_view_all"
              i18n-name-key="webfragments.navigation.bar.project.item.view.all" system="true" weight="10">
        <label key="menu.project.view.all"/>
        <tooltip key="menu.project.view.all.title"/>
        <link linkId="project_view_all_link">/secure/BrowseProjects.jspa#all</link>
    </web-item>


    <!--Issue Menu-->

    <!-- if there are any projects the user can browse - show them the find link -->
    <web-item key="find_link" name="Find Issues Link" section="system.top.navigation.bar"
              i18n-name-key="webfragments.navigation.bar.item.find.issues.name" system="true" weight="30">
        <label key="menu.issues.header"/>
        <tooltip key="menu.issues.header.title"/>
        <link linkId="find_link" accessKey="$authcontext.i18nBean.getText('alt.text.findissues.accessKey')">
            /secure/IssueNavigator.jspa
        </link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserHasVisibleProjectsCondition">
            <param name="permission">browse</param>
        </condition>
    </web-item>

    <web-section key="issues_current_main" name="Issues dropdown Current Search Section" location="find_link"
                 i18n-name-key="webfragments.navigation.bar.issues.dropdown.current" system="true" weight="10">
        <label key="menu.issues.current.section"/>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.HasLastSearchRequestCondition"/>
    </web-section>

    <simple-link-factory key="issues_drop_current-factory" name="Current Search Link Factory"
                         section="find_link/issues_current_main"
                         i18n-name-key="webfragments.navigation.bar.issues.current.link.factory" system="true"
                         weight="20" lazy="true"
                         class="com.atlassian.jira.issue.search.CurrentSearchLinkFactory"/>

    <web-section key="issues_new" name="New Search Dropdown Section" location="find_link"
                 i18n-name-key="webfragments.navigation.bar.issues.dropdown.new" system="true" weight="20">
    </web-section>

    <web-item key="issues_new_search_link" name="New Search Link" section="find_link/issues_new"
              i18n-name-key="webfragments.navigation.bar.issues.item.new.filter" system="true" weight="10">
        <label key="menu.issues.new.search"/>
        <tooltip key="menu.issues.new.search.title"/>
        <link linkId="issues_new_search_link">/secure/IssueNavigator.jspa?mode=show&amp;createNew=true</link>
    </web-item>

    <web-item key="issues_new_issue_link" name="New Issue Menu Link" section="find_link/issues_new"
              i18n-name-key="webfragments.navigation.bar.issues.item.new.issue" system="true" weight="20">
        <label key="menu.issues.new.issue"/>
        <tooltip key="menu.issues.new.issue.title"/>
        <link linkId="issues_new_issue_link">/secure/CreateIssue!default.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserHasVisibleProjectsCondition">
            <param name="permission">create</param>
        </condition>
    </web-item>

    <web-section key="issues_history_main" name="Issues dropdown history Section" location="find_link"
                 i18n-name-key="webfragments.navigation.bar.issues.dropdown.history" system="true" weight="30">
        <label key="menu.issues.history.section"/>
    </web-section>

    <simple-link-factory key="issues_drop_history-factory" name="Issue History Link Factory"
                         section="find_link/issues_history_main"
                         i18n-name-key="webfragments.navigation.bar.issues.history.link.factory" system="true"
                         weight="20" lazy="true"
                         class="com.atlassian.jira.issue.history.IssueHistoryLinkFactory"/>

    <web-section key="issues_filter_main" name="Issues dropdown filter Section" location="find_link"
                 i18n-name-key="webfragments.navigation.bar.issues.dropdown.filters" system="true" weight="50">
        <label key="menu.issues.filter.section"/>
    </web-section>

    <simple-link-factory key="issues_drop_fav-filters-factory" name="Favourite Filters Link Factory"
                         section="find_link/issues_filter_main"
                         i18n-name-key="webfragments.navigation.bar.issues.fav.filters.link.factory" system="true"
                         weight="20" lazy="true"
                         class="com.atlassian.jira.issue.search.FavouriteFilterLinkFactory"/>


    <web-section key="issues_manage_filters" name="Manage filters Dropdown Section" location="find_link"
                 i18n-name-key="webfragments.navigation.bar.issues.dropdown.manage" system="true" weight="90"/>

    <web-item key="issues_manage_filters_link" name="Manage Filters Link" section="find_link/issues_manage_filters"
              i18n-name-key="webfragments.navigation.bar.issues.item.manage.filters" system="true" weight="10">
        <label key="menu.issues.manage.filters"/>
        <tooltip key="menu.issues.manage.filters.title"/>
        <link linkId="issues_manage_filters_link">/secure/ManageFilters.jspa</link>
    </web-item>


    <!--Admin menu-->

    <!-- if the user is an administrator or project admin - show them the administration link -->
    <web-item key="admin_link" name="Administration Link" section="system.top.navigation.bar"
              i18n-name-key="webfragments.navigation.bar.item.admin.name" system="true" weight="50">
        <label key="menu.admin.header"/>
        <tooltip key="menu.admin.header.title"/>
        <link linkId="admin_link">/secure/project/ViewProjects.jspa</link>
        <conditions type="OR">
            <condition class="com.atlassian.jira.plugin.webfragment.conditions.JiraGlobalPermissionCondition">
                <param name="permission">admin</param>
            </condition>
            <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserIsProjectAdminCondition"/>
        </conditions>
    </web-item>


    <web-section key="admin_main" name="Admin Dropdown" location="admin_link"
                 i18n-name-key="webfragments.navigation.bar.admin.dropdown.main" system="true" weight="100"/>

    <web-item key="admin_mac_link" name="Administration Dropdown My Atlassian Account Link"
              section="admin_link/admin_main" i18n-name-key="webfragments.navigation.bar.admin.item.mac" system="true"
              weight="20">
        <label key="menu.admin.mac"/>
        <tooltip key="menu.admin.mac.title"/>
        <link linkId="admin_mac_link">http://www.atlassian.com/my/</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.JiraGlobalPermissionCondition">
            <param name="permission">sysadmin</param>
        </condition>
    </web-item>
    <web-item key="admin_plugin_exchange_link" name="Administration Dropdown Plugin Exchange Link"
              section="admin_link/admin_main" i18n-name-key="webfragments.navigation.bar.admin.item.plugin.exchange"
              system="true" weight="22">
        <label key="menu.admin.plugin"/>
        <tooltip key="menu.admin.plugin.title"/>
        <link linkId="admin_plugin_exchange_link">https://plugins.atlassian.com/search/by/jira</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.JiraGlobalPermissionCondition">
            <param name="permission">sysadmin</param>
        </condition>
    </web-item>
    <web-item key="admin_main_proj_link" name="Administrate Project Dropdown Link" section="admin_link/admin_main"
              i18n-name-key="webfragments.navigation.bar.admin.item.project" system="true" weight="30">
        <label>$authcontext.i18nBean.getText('menu.admin.current.project',
            $textutils.htmlEncode($helper.project.get('name')))
        </label>
        <tooltip key="menu.admin.current.project.title"/>
        <link linkId="admin_main_proj_link">/secure/project/ViewProject.jspa?pid=${helper.project.get('id')}</link>
        <icon width="16" height="16">
            <link linkId="admin_main_proj_link">/secure/projectavatar?pid=${helper.projectObject.id}&amp;avatarId=${helper.projectObject.avatar.id}&amp;size=small</link>
        </icon>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.HasSelectedProjectCondition"/>
    </web-item>

    <!--<web-section key="admin_other" name="Other Admin Tasks Dropdown" location="admin_link" i18n-name-key="webfragments.navigation.bar.admin.dropdown.other" system="true" weight="200">-->
    <!--</web-section>-->

    <!--<web-item key="admin_other_sys_info_link" name="System Info Dropdown Link" section="admin_link/admin_other" i18n-name-key="webfragments.navigation.bar.admin.item.sysinfo" system="true" weight="10">-->
    <!--<label key="menu.admin.system.info" />-->
    <!--<tooltip key="menu.admin.system.info.title" />-->
    <!--<link linkId="admin_other_sys_info_link">/secure/admin/jira/ViewSystemInfo.jspa</link>-->
    <!--</web-item>-->


    <web-section key="tasks" name="Admin Tasks Section" location="admin_link"
                 i18n-name-key="webfragments.menu.section.tasks.name" state="enabled" weight="100">
        <label key="admin.menu.tasks"/>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.JiraGlobalPermissionCondition">
            <param name="permission">admin</param>
        </condition>
    </web-section>

    <web-item key="view_projects" name="View Projects Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.view.projects.name" state="enabled" weight="10">
        <label key="common.concepts.projects"/>
        <link linkId="view_projects">/secure/project/ViewProjects.jspa</link>
    </web-item>

    <web-item key="view_categories" name="View Project Categories Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.view.categories.name" state="disabled" weight="15">
        <label key="admin.menu.projects.project.categories"/>
        <link linkId="view_categories">/secure/admin/projectcategories/ViewProjectCategories!default.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.EnterpriseJiraLicenseCondition"/>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserIsAdminCondition"/>
    </web-item>

    <web-item key="userbrowser" name="User Browser Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.userbrowser.name" state="enabled" weight="20">
        <label key="admin.menu.usersandgroups.user.browser"/>
        <link linkId="user_browser">/secure/admin/user/UserBrowser.jspa</link>
    </web-item>

    <web-item key="groupbrowser" name="Group Browser Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.groupbrowser.name" state="disabled" weight="25">
        <label key="admin.menu.usersandgroups.group.browser"/>
        <link linkId="group_browser">/secure/admin/user/GroupBrowser.jspa</link>
    </web-item>

    <web-item key="projectrolebrowser" name="Project Role Browser Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.projectrolebrowser.name" state="disabled" weight="30">
        <label key="admin.menu.usersandgroups.role.browser"/>
        <link linkId="project_role_browser">/secure/project/ViewProjectRoles.jspa</link>
    </web-item>

    <web-item key="attachments" name="Attachments Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.attachments.name" state="disabled" weight="35">
        <label key="admin.menu.globalsettings.attachments"/>
        <link linkId="attachments">/secure/admin/jira/ViewAttachmentSettings.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserIsAdminCondition"/>
    </web-item>

    <web-item key="cvs_modules" name="CVS Modules Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.cvs.modules.name" state="disabled" weight="40">
        <label key="admin.menu.globalsettings.cvs.modules"/>
        <link linkId="cvs_modules">/secure/admin/ViewRepositories.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserIsAdminCondition"/>
    </web-item>

    <web-item key="edit_default_dashboard" name="Edit Default Dashboard Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.edit.default.dashboard.name" state="disabled" weight="45">
        <label key="admin.menu.globalsettings.default.dashboard"/>
        <link linkId="edit_default_dashboard">/secure/admin/jira/EditDefaultDashboard!default.jspa</link>
    </web-item>

    <web-item key="eventtypes" name="Event Types Link" section="admin_link/globalsettings"
              i18n-name-key="webfragments.admin.menu.item.eventtypes.name" state="disabled" weight="50">
        <label key="admin.menu.globalsettings.event.types"/>
        <link linkId="eventtypes">/secure/admin/jira/ListEventTypes.jspa</link>
        <condition
                class="com.atlassian.jira.plugin.webfragment.conditions.ProfessionalOrEnterpriseJiraLicenseCondition"/>
    </web-item>

    <web-item key="general_configuration" name="General Configuration Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.general.configuration.name" state="enabled" weight="55">
        <label key="admin.menu.globalsettings.general.configuration"/>
        <link linkId="general_configuration">/secure/admin/jira/ViewApplicationProperties.jspa</link>
    </web-item>

    <web-item key="global_permissions" name="Global Permissions Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.global.permissions.name" state="disabled" weight="60">
        <label key="admin.menu.globalsettings.global.permissions"/>
        <link linkId="global_permissions">/secure/admin/jira/GlobalPermissions!default.jspa</link>
    </web-item>

    <web-item key="linking" name="Issue Linking Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.linking.name" state="disabled" weight="70">
        <label key="admin.menu.globalsettings.issue.linking"/>
        <link linkId="linking">/secure/admin/jira/ViewLinkTypes.jspa</link>
    </web-item>

    <web-item key="lookandfeel" name="Look and Feel Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.lookandfeel.name" state="enabled" weight="80">
        <label key="admin.menu.globalsettings.look.and.feel"/>
        <link linkId="lookandfeel">/secure/admin/jira/ViewLookAndFeel.jspa</link>
    </web-item>

    <web-item key="mail_servers" name="Mail Servers Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.mail.servers.name" state="disabled" weight="90">
        <label key="admin.menu.globalsettings.mail.servers"/>
        <link linkId="mail_servers">/secure/admin/ViewMailServers.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserIsSysAdminCondition"/>
    </web-item>

    <web-item key="subtasks" name="Subtasks Link" section="admin_link/globalsettings"
              i18n-name-key="webfragments.admin.menu.item.subtasks.name" state="disabled" weight="100">
        <label key="admin.menu.globalsettings.sub.tasks"/>
        <link linkId="subtasks">/secure/admin/subtasks/ManageSubTasks.jspa</link>
        <condition
                class="com.atlassian.jira.plugin.webfragment.conditions.ProfessionalOrEnterpriseJiraLicenseCondition"/>
    </web-item>

    <web-item key="timetracking" name="Timetracking Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.timetracking.name" state="disabled" weight="110">
        <label key="admin.menu.globalsettings.time.tracking"/>
        <link linkId="timetracking">/secure/admin/jira/TimeTrackingAdmin!default.jspa</link>
    </web-item>

    <web-item key="trackbacks" name="Trackbacks Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.trackbacks.name" state="disabled" weight="120">
        <label key="admin.menu.globalsettings.trackbacks"/>
        <link linkId="trackbacks">/secure/admin/jira/TrackbackAdmin.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.UserIsSysAdminCondition"/>
    </web-item>

    <web-item key="user_defaults" name="User Defaults Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.user.defaults.name" state="disabled" weight="130">
        <label key="admin.menu.globalsettings.user.defaults"/>
        <link linkId="user_defaults">/secure/admin/jira/ViewUserDefaultSettings.jspa</link>
    </web-item>

    <web-item key="workflows" name="Workflows Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.workflows.name" state="disabled" weight="140">
        <label key="admin.menu.globalsettings.workflows"/>
        <link linkId="workflows">/secure/admin/workflows/ListWorkflows.jspa</link>
        <condition
                class="com.atlassian.jira.plugin.webfragment.conditions.ProfessionalOrEnterpriseJiraLicenseCondition"/>
    </web-item>

    <web-item key="security_schemes" name="Security Schemes Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.security.schemes.name" state="disabled" weight="150">
        <label key="admin.menu.schemes.issue.security.schemes"/>
        <link linkId="security_schemes">/secure/admin/ViewIssueSecuritySchemes.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.EnterpriseJiraLicenseCondition"/>
    </web-item>

    <web-item key="notification_schemes" name="Notification Schemes Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.notification.schemes.name" state="disabled" weight="160">
        <label key="admin.menu.schemes.notification.schemes"/>
        <link linkId="notification_schemes">/secure/admin/ViewNotificationSchemes.jspa</link>
    </web-item>

    <web-item key="permission_schemes" name="Permission Schemes Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.permission.schemes.name" state="disabled" weight="170">
        <label key="admin.menu.schemes.permission.schemes"/>
        <link linkId="permission_schemes">/secure/admin/ViewPermissionSchemes.jspa</link>
    </web-item>

    <web-item key="workflow_schemes" name="Workflow Schemes Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.workflow.schemes.name" state="disabled" weight="180">
        <label key="admin.menu.schemes.workflow.schemes"/>
        <link linkId="workflow_schemes">/secure/admin/ViewWorkflowSchemes.jspa</link>
        <condition class="com.atlassian.jira.plugin.webfragment.conditions.EnterpriseJiraLicenseCondition"/>
    </web-item>

    <web-item key="scheme_tools" name="Scheme Tools Link" section="admin_link/tasks"
              i18n-name-key="webfragments.admin.menu.item.scheme.tools.name" state="disabled" weight="190">
        <label key="admin.menu.scheme.tools"/>
